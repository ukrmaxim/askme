<div class="content-wrapper buffer-both">
  <div class="buffer-both">
    <h1 class="title-medium"><%= t('questions_with_hashtag') %><%= @hashtag.name %></h1>
  </div>
  <div class="user-questions buffer-both">
    <% @hashtag.questions.each do |question| %>
      <%= render partial: 'questions/question', object: question %>
    <% end %>
  </div>
</div>
